{
    "name": "macavity/webpack-filter",
    "type": "symfony-bundle",
    "description": "An intermediate AsseticFilter to use Webpack with Assetic as an intermediate solution for the switch to webpack-encore.",
    "keywords": ["assetic", "SASS", "SCSS", "babel", "webpack"],
    "license": "MIT",
    "authors": [
        {
            "name": "Macavity",
            "homepage": "https://github.com/macavity"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "php": ">=5.3.2",
        "kriswallsmith/assetic": "^1.3"
    },
    "autoload": {
        "psr-4": { "Macavity\\Assetic\\": "" }
    }
}
